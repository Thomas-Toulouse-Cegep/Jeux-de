using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Jeuxdée
{
    internal abstract class Controleur
    {
        private int nbFace;
        private string type;
        private int chance;

        public int NbFace { get => nbFace; set => nbFace = value; }
        public string Type { get => type; set => type = value; }

        public abstract void ImageDe(PictureBox pictureBox, Graphics graphics);
    }
}